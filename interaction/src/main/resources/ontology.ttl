ids:ArtifactState  rdf:type  owl:Class ;
        rdfs:label    "Artifact State"@en ;
        rdfs:comment  "The state which an ids:Artifact may have or not."@en .
@prefix dct:   <http://purl.org/dc/terms/> .
@prefix dcat:  <http://www.w3.org/ns/dcat#> .
