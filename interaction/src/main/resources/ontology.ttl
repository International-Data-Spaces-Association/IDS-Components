        ids:includedCertificationLevel  idsc:COMPONENT_BASE_SECURITY_PROFILE_CHECKLIST_APPROACH .

idsc:COMPONENT_TRUST_SECURITY_PROFILE_CONCEPT_REVIEW
        rdf:type                        ids:ComponentCertificationLevel ;
