@prefix ids:   <https://w3id.org/idsa/core/> .

ids:Language  rdf:type  owl:Class ;
        rdfs:label    "Language"@en ;
        rdfs:comment  "The class of languages potentially being used by textual Data Assets."@en .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct:   <http://purl.org/dc/terms/> .
@prefix ids:   <https://w3id.org/idsa/core/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix idsc:  <https://w3id.org/idsa/code/> .

ids:CustomMediaType  rdf:type  owl:Class ;
        rdfs:subClassOf  dct:MediaType ;
        rdfs:label       "Custom Media Type"@en ;
        rdfs:comment     "A selection of custom media types to be used for data published on the IDS when no IANA type is available."@en .

ids:IANAMediaType  rdf:type  owl:Class ;
        rdfs:subClassOf  dct:MediaType ;
        rdfs:label       "IANA Media Type"@en ;
        rdfs:comment     "The class of media types registered with IANA."@en ;
        rdfs:seeAlso     <https://www.iana.org/assignments/media-types/> ;
        rdfs:seeAlso     <http://www.sparontologies.net/mediatype/> ;
        rdfs:seeAlso     <http://dx.doi.org/10.6084/m9.figshare.1608294> .

ids:filenameExtension
        rdf:type      owl:DatatypeProperty ;
        rdfs:label    "filename extension"@en ;
        rdfs:domain   dct:MediaType ;
        rdfs:range    xsd:string ;
        rdfs:comment  "Suffix of a file name, typically separated by a period, indicating the nature and intended processing of the file."@en ;
        rdfs:seeAlso  <https://en.wikipedia.org/wiki/Filename_extension> .
@prefix dct:   <http://purl.org/dc/terms/> .
@prefix ids:   <https://w3id.org/idsa/core/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .

ids:PaymentModality  rdf:type  owl:Class ;
        rdfs:label    "Payment Modality"@en ;
        rdfs:comment  "Various pricing modalities such as idsc:FREE, idsc:FIXED_PRICE and idsc:NEGOTIATION_BASIS can be used as a pre-selection to annotate the terms of the contract. Depending on the requirements, additional free text information can be attached."@en .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct:   <http://purl.org/dc/terms/> .
@prefix dcat:  <http://www.w3.org/ns/dcat#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix ids:   <https://w3id.org/idsa/core/> .

ids:RepresentationInstance
        rdf:type      owl:Class ;
        rdfs:label    "Representation instance"@en ;
        rdfs:comment  "A particular instance of given representation."@en .

ids:Value  rdf:type       owl:Class ;
        rdfs:subClassOf   ids:RepresentationInstance ;
        rdfs:label        "Value"@en ;
        rdfs:comment      "Literal, inline value instantiation of a representation."@en ;
        owl:disjointWith  ids:Artifact .

ids:instance  rdf:type  owl:ObjectProperty ;
        rdfs:domain   dcat:Distribution ;
        rdfs:range    ids:RepresentationInstance ;
        rdfs:label    "instance"@en ;
        rdfs:comment  "Reference to an instance of given representation, i.e. inline value or file placeholder."@en .

ids:representationStandard
        rdf:type            owl:DatatypeProperty ;
        rdfs:subPropertyOf  dct:conformsTo ;
        rdfs:domain         dcat:Distribution ;
        rdfs:range          xsd:anyURI ;
        rdfs:label          "representation standard"@en ;
        rdfs:comment        "Standards document defining the given Representation (in contrast to general Resource content). The Representation is assumed to conform to that Standard."@en .
@prefix dct:   <http://purl.org/dc/terms/> .
@prefix ids:   <https://w3id.org/idsa/core/> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix odrl:  <http://www.w3.org/ns/odrl/2/> .

ids:Resource  rdf:type   owl:Class ;
        rdfs:subClassOf  ids:DescribedSemantically ;
        rdfs:subClassOf  ids:DigitalContent ;
        rdfs:subClassOf  ids:ManagedEntity ;
        rdfs:subClassOf  odrl:Asset ;
        rdfs:label       "Resource"@en ;
        rdfs:comment     "Resource is a single digital content or a coherent set of digital contents. Resource content is formalized in Representations and optionally materialized as Artifacts. The Resource's content is exposed via defined Interfaces at various protocol Endpoints."@en ;
        rdfs:seeAlso     <https://www.w3.org/TR/vocab-dcat/#class-dataset> .

ids:resourcePart  rdf:type  owl:ObjectProperty ;
        rdfs:subPropertyOf  ids:contentPart ;
        rdfs:label          "resource part"@en ;
        rdfs:domain         ids:Resource ;
        rdfs:range          ids:Resource ;
        rdfs:comment        "Reference to a Resource (physically or logically) included, definition of part-whole hierarchies."@en .

ids:resourceEndpoint  rdf:type  owl:ObjectProperty ;
        rdfs:label    "resource endpoint"@en ;
        rdfs:domain   ids:Resource ;
        rdfs:range    ids:ConnectorEndpoint ;
        rdfs:comment  "Reference to the Endpoints that serve the resource's content or let you exchange messages with an IDS Connector."@en .

ids:contractOffer  rdf:type  owl:ObjectProperty ;
        rdfs:domain   ids:Resource ;
        rdfs:range    ids:ContractOffer ;
        rdfs:label    "contract offer"@en ;
        rdfs:comment  "Reference to a Contract Offer defining the authorized use of the Resource."@en .

ids:paymentModality  rdf:type  owl:ObjectProperty ;
        rdfs:domain   ids:Resource ;
        rdfs:range    ids:PaymentModality ;
        rdfs:label    "payment modality"@en ;
        rdfs:comment  "Various pricing modalities such as idsc:FREE, idsc:FIXED_PRICE and idsc:NEGOTIATION_BASIS can be used as a pre-selection to annotate the terms of the contract. Depending on the requirements, additional free text information can be attached."@en .

ids:sovereign  rdf:type  owl:ObjectProperty ;
        rdfs:domain   ids:Resource ;
        rdfs:range    foaf:Agent ;
        rdfs:label    "sovereign"@en ;
        rdfs:comment  "The 'owner', i.e. sovereign of the data."@en .

ids:sample  rdf:type  owl:ObjectProperty ;
        rdfs:label    "sample"@en ;
        rdfs:domain   ids:Resource ;
        rdfs:range    ids:Resource ;
        rdfs:comment  "Sample Resource instance."@en .

ids:variant  rdf:type  owl:ObjectProperty ;
        rdfs:label    "variant"@en ;
        rdfs:domain   ids:Resource ;
        rdfs:range    ids:Resource ;
        rdfs:comment  "(Equivalent) variant of given Resource, e.g., a translation."@en .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct:   <http://purl.org/dc/terms/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix odrl:  <http://www.w3.org/ns/odrl/2/> .
@prefix ids:   <https://w3id.org/idsa/core/> .

ids:UsageControlObject
        rdf:type      owl:Class ;
        rdfs:label    "Usage Control Object"@en ;
        rdfs:comment  "A data element containing all relevant information for usage control engines."@en .

ids:data  rdf:type    owl:ObjectProperty ;
        rdfs:label    "has data"@en ;
        rdfs:domain   ids:UsageControlObject ;
        rdfs:range    xsd:anyURI ;
        rdfs:comment  "The related data or a pointer/reference to it."@en .

ids:user  rdf:type    owl:ObjectProperty ;
        rdfs:label    "has user"@en ;
        rdfs:domain   ids:UsageControlObject ;
        rdfs:range    ids:Participant ;
        rdfs:comment  "The participant intending to use the data."@en .

ids:source  rdf:type  owl:ObjectProperty ;
        rdfs:label    "source"@en ;
        rdfs:domain   ids:UsageControlObject ;
        rdfs:range    xsd:anyURI ;
        rdfs:comment  "The application which provided the data object. Should be a valid URI. Must be unique in the regarded environment."@en .

ids:contract  rdf:type  owl:ObjectProperty ;
        rdfs:label    "has contract"@en ;
        rdfs:domain   ids:UsageControlObject ;
        rdfs:range    ids:ContractAgreement ;
        rdfs:comment  "The target contract defining the usage of the data object."@en .

ids:usageDuration  rdf:type  owl:ObjectProperty ;
        rdfs:label    "usage duration"@en ;
        rdfs:domain   ids:UsageControlObject ;
        rdfs:range    xsd:duration ;
        rdfs:comment  "Duration, which this asset is in use by now. Might be calculated on the fly when requested by a Usage Control Engine."@en .

ids:accessed  rdf:type  owl:ObjectProperty ;
        rdfs:label    "last accessed"@en ;
        rdfs:domain   ids:UsageControlObject ;
        rdfs:range    xsd:dateTimeStamp ;
        rdfs:comment  "Last read or write access."@en .

ids:hasState  rdf:type  owl:ObjectProperty ;
        rdfs:label    "has state"@en ;
        rdfs:domain   ids:UsageControlObject ;
        rdfs:range    ids:ArtifactState ;
        rdfs:comment  "The list of combined current states of the data object."@en .

ids:requester  rdf:type  owl:ObjectProperty ;
        rdfs:label    "requesting application"@en ;
        rdfs:domain   ids:UsageControlObject ;
        rdfs:range    xsd:anyURI ;
        rdfs:comment  "\tThe target application requesting the usage. Should be some locally unique identifiers, preferably URIs."@en .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix ids:   <https://w3id.org/idsa/core/> .
@prefix dct:   <http://purl.org/dc/terms/> .

ids:BusinessIdentifier
        rdf:type      owl:Class ;
        rdfs:label    "Business Identifier"@en ;
        rdfs:comment  "A business identifier is a identification number used to identify a company. E.g., the Data Universal Numbering System, abbreviated as DUNS"@en .

ids:identifierSystem  rdf:type  owl:DatatypeProperty ;
        rdfs:domain   ids:BusinessIdentifier ;
        rdfs:range    xsd:string ;
        rdfs:label    "identifier system"@en ;
        rdfs:comment  "This property is used to define the system used as an business identifier. E.g., DUNS or SIRET."@en .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix org:   <http://www.w3.org/ns/org#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix odrl:  <http://www.w3.org/ns/odrl/2/> .
@prefix ids:   <https://w3id.org/idsa/core/> .

ids:Participant  rdf:type  owl:Class ;
        rdfs:subClassOf  foaf:Agent ;
        rdfs:subClassOf  odrl:Party ;
        rdfs:subClassOf  ids:ManagedEntity ;
        rdfs:subClassOf  org:Organization ;
        rdfs:label       "Participant"@en ;
        rdfs:comment     "Stakeholder in the International Data Spaces, assuming one or more of the predefined roles; every participant is given a unique identity by the Identity Provider."@en .

ids:legalName  rdf:type  owl:DatatypeProperty ;
        rdfs:domain   ids:Participant ;
        rdfs:range    xsd:string ;
        rdfs:label    "legal name"@en ;
        rdfs:comment  "The complete legal name of a Participant. Unlike the title property for the participant which refers to the commonly used name or term for the Participant, a legal name identifies a participant for legal, administrative and other official purposes."@en .

ids:memberParticipant
        rdfs:subPropertyOf  org:hasMember ;
        rdf:type            owl:ObjectProperty ;
        rdfs:domain         ids:Participant ;
        rdfs:range          ids:Participant ;
        rdfs:label          "member participant"@en ;
        rdfs:comment        "Indicates that a participant has a member which is again a participant. This is useful for defining hierarchical relations in a participant's organization as well as identifying groups of participants to capture, e.g., members of a collaboration."@en .

ids:participantCertification
        rdf:type      owl:ObjectProperty ;
        rdfs:label    "participant certification"@en ;
        rdfs:domain   ids:Participant ;
        rdfs:range    ids:ParticipantCertification ;
        rdfs:comment  "Certification issued for the given Participant."@en .

ids:participantRefinement
        rdf:type      owl:ObjectProperty ;
        rdfs:label    "participant refinement"@en ;
        rdfs:domain   ids:Participant ;
        rdfs:range    odrl:Constraint ;
        rdfs:comment  "Conditions which needs to be satisfied that a single Participant can be seen as a member of the subject Participant. For instance, all Participants with their headquarter in Europe might be a potential consumer of GDPR-releated data."@en .

ids:businessIdentifier
        rdf:type      owl:ObjectProperty ;
        rdfs:domain   ids:Participant ;
        rdfs:range    ids:BusinessIdentifier ;
        rdfs:label    "has business identifier"@en ;
        rdfs:comment  "Company identifiers, described via ids:BusinessIdentifier."@en .

ids:vatID  rdf:type   owl:DatatypeProperty ;
        rdfs:domain   ids:Participant ;
        rdfs:range    xsd:string ;
        rdfs:label    "VAT ID"@en ;
        rdfs:comment  "The Value Added Tax ID (VAT ID) is used in the EU to clearly identify a company or entity which is liable for Value Added Tax."@en .

ids:legalForm  rdf:type  owl:DatatypeProperty ;
        rdfs:domain   ids:Participant ;
        rdfs:range    xsd:string ;
        rdfs:label    "legal form"@en ;
        rdfs:seeAlso  <https://www.gleif.org/en/about-lei/code-lists/iso-20275-entity-legal-forms-code-list/> ;
        rdfs:comment  "Legal form of the participant. It is advisable to use an existing standard such as ISO-20275."@en .

ids:jurisdiction  rdf:type  owl:DatatypeProperty ;
        rdfs:domain   ids:Participant ;
        rdfs:range    xsd:string ;
        rdfs:label    "legal form"@en ;
        rdfs:comment  "jurisdiction / court which is responsible for the entity. The use of existing standards such as ISO 3166-2 is recommended."@en .

ids:memberPerson  rdfs:subPropertyOf  org:hasMember ;
        rdf:type            owl:ObjectProperty ;
        rdfs:domain         ids:Participant ;
        rdfs:range          foaf:Person ;
        rdfs:label          "memberPerson"@en ;
        rdfs:comment        "Indicates membership of a person to an organization."@en .

ids:siteAddress  rdf:type  owl:DatatypeProperty ;
        rdfs:domain   org:Site ;
        rdfs:range    xsd:string ;
        rdfs:label    "site address"@en ;
        rdfs:comment  "Indicates an address for the site."@en .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct:   <http://purl.org/dc/terms/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix ids:   <https://w3id.org/idsa/core/> .

ids:AuthInfo  rdf:type  owl:Class ;
        rdfs:label    "AuthInfo"@en ;
        rdfs:comment  "Holds authentication information so that connections to the Connector's data offerings can be establised."@en .

ids:authService  rdf:type  owl:ObjectProperty ;
        rdfs:domain   ids:AuthInfo ;
        rdfs:range    xsd:anyURI ;
        rdfs:label    "authService"@en ;
        rdfs:comment  "The URL of the authentication service that is provided by the Connector."@en .

ids:authStandard  rdf:type  owl:ObjectProperty ;
        rdfs:domain   ids:AuthInfo ;
        rdfs:range    ids:AuthStandard ;
        rdfs:label    "authStandard"@en ;
        rdfs:comment  "The standard used by the authentication service that is provided by the Connector."@en .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct:   <http://purl.org/dc/terms/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix ids:   <https://w3id.org/idsa/core/> .
@prefix idsc:  <https://w3id.org/idsa/code/> .

ids:AuthStandard  rdf:type  owl:Class ;
        rdfs:label    "AuthStandard"@en ;
        rdfs:comment  "The class of authentication standards that may be supported by Connectors."@en .

idsc:OAUTH2_JWT  rdf:type  ids:AuthStandard ;
        rdfs:comment  "OAUTH2 via JWT" .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix dct:   <http://purl.org/dc/terms/> .
@prefix ids:   <https://w3id.org/idsa/core/> .

ids:SecurityProfile  rdf:type  owl:Class ;
        rdfs:label    "SecurityProfile"@en ;
        rdfs:comment  "Set of security guarantees claimed by a Connector. Connectors may asses their mutual technical reliability and trustworthiness by evaluating each other’s security profile."@en .

ids:SecurityGuarantee
        rdf:type      owl:Class ;
        rdfs:label    "Security guarantee"@en ;
        rdfs:comment  "Security guarantee claimed by a security profile."@en .

ids:securityGuarantee
        rdf:type      owl:ObjectProperty ;
        rdfs:domain   ids:SecurityProfile ;
        rdfs:range    ids:SecurityGuarantee ;
        rdfs:label    "Security guarantee"@en ;
        rdfs:comment  "Reference to a security guarantee supported by given profile."@en .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct:   <http://purl.org/dc/terms/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix ids:   <https://w3id.org/idsa/core/> .

ids:Token  rdf:type   owl:Class ;
        rdfs:label    "Token."@en ;
        rdfs:comment  "A token (e.g., for authorization) mediated as part of the message."@en .

ids:DynamicAttributeToken
        rdf:type         owl:Class ;
        rdfs:subClassOf  ids:Token ;
        rdfs:label       "Dynamic Attribute Token (DAT)."@en ;
        rdfs:comment     "A JSON Web Token (e.g., for authorization) mediated as part of the message, formerly known as the 'DAPS Token'. The complete JWT *including* header, payload and signiture is referenced by this class and *not* only the JSON element in the payload. See ids:DatPayload for the payload definition."@en ;
        rdfs:seeAlso     <https://tools.ietf.org/html/rfc7519> ;
        rdfs:seeAlso     ids:DatPayload .

ids:TokenFormat  rdf:type  owl:Class ;
        rdfs:label    "Token format"@en ;
        rdfs:comment  "Possible formats for (security-related) tokens."@en .

ids:JwtPayload  rdf:type  owl:Class ;
        rdfs:label    "JWT Payload"@en ;
        rdfs:comment  "The payload of an JSON Web Token as a RDF class. Is used as the common parent of e.g., DatPayload and DatRequestPayload."@en .

ids:transportCertsSha256
        rdf:type      owl:DatatypeProperty ;
        rdfs:domain   ids:JwtPayload ;
        rdfs:range    xsd:string ;
        rdfs:label    "transportCertsSha256"@en ;
        rdfs:comment  "Separate certificates for IDS identification and transport encryption opens an attack vector for relay attacks. In order to prevent these attacks, a binding of this transport certificates to the connector is required. The inclusion of SHA256 fingerprints of currently valid transport certificates, mainly into the DAT, enables the client to relate the transport layer security with the IDS interactions."@en .

ids:DatPayload  rdf:type  owl:Class ;
        rdfs:subClassOf  ids:JwtPayload ;
        rdfs:label       "DAT Payload"@en ;
        rdfs:comment     "The Dynamic Attribute Token (DAT) Payload is the JSON Element of any DAT containing the claims of the token bearer. This payload itself is a JSON-LD encoded RDF class with a defined set of attributes. These attributes are either defined by RFC 7519 or by the IDS Information Model. As a direct consequence of regarding the DAT Payload as JSON-LD, *all* DAT Payloads must have exactly one \"@context\" attribute with the IDS context URI as its value and a \"@type\" with ids:DatPayload as its value."@en ;
        rdfs:seeAlso     <https://tools.ietf.org/html/rfc7519> .

ids:DatRequestPayload
        rdf:type         owl:Class ;
        rdfs:subClassOf  ids:JwtPayload ;
        rdfs:label       "DAT Request Payload"@en ;
        rdfs:comment     "The Dynamic Attribute Token (DAT) *Request* Payload is the JSON Element of any DAT containing the claims of the token bearer. This payload itself is a JSON-LD encoded RDF class with a defined set of attributes. These attributes are either defined by RFC 7519 or by the IDS Information Model. As a direct consequence of regarding the DAT Payload as JSON-LD, *all* DAT Payloads must have exactly one \"@context\" attribute with the IDS context URI as its value and a \"@type\" with ids:DatPayload as its value. Note that, different to the DatPayload, the DatRequestPayload contains the *self-claims* of a connector and is not yet signed by any DAPS. Consequently, no other connector must accept a DatRequest object as a DAT. A DatRequest and its contained DatRequestPayload is *only* intended for interactions with a DAPS and *nothing else*!"@en ;
        rdfs:seeAlso     <https://tools.ietf.org/html/rfc7519> .

ids:Audience  rdf:type  owl:Class ;
        rdfs:label    "Audience"@en ;
        rdfs:comment  "The class of audiences (recipients) used in the JWT. "@en ;
        rdfs:seeAlso  <https://tools.ietf.org/html/rfc7519> .

ids:aud  rdf:type     owl:ObjectProperty ;
        rdfs:label    "aud"@en ;
        rdfs:domain   ids:JwtPayload ;
        rdfs:range    ids:Audience ;
        rdfs:seeAlso  <https://tools.ietf.org/html/rfc7519#section-4.1.3> ;
        rdfs:comment  "The 'aud' (audience) claim identifies the recipients that the JWT is intended for."@en .

ids:exp  rdf:type     owl:DatatypeProperty ;
        rdfs:label    "exp"@en ;
        rdfs:domain   ids:JwtPayload ;
        rdfs:range    xsd:integer ;
        rdfs:seeAlso  <https://tools.ietf.org/html/rfc7519#section-4.1.4> ;
        rdfs:comment  "The 'exp' (expiration time) claim identifies the expiration time on or after which the JWT MUST NOT be accepted for processing."@en .

ids:iat  rdf:type     owl:DatatypeProperty ;
        rdfs:label    "iat"@en ;
        rdfs:domain   ids:JwtPayload ;
        rdfs:range    xsd:integer ;
        rdfs:comment  "The 'iat' (issued at) claim contains the point in time when the JWT was created."@en .

ids:iss  rdf:type     owl:DatatypeProperty ;
        rdfs:label    "iss"@en ;
        rdfs:domain   ids:JwtPayload ;
        rdfs:range    xsd:string ;
        rdfs:seeAlso  <https://tools.ietf.org/html/rfc7519#section-4.1.1> ;
        rdfs:comment  "The 'iss' (issuer) claim identifies the principal that issued the JWT."@en .

ids:nbf  rdf:type     owl:DatatypeProperty ;
        rdfs:label    "nbf"@en ;
        rdfs:domain   ids:JwtPayload ;
        rdfs:range    xsd:integer ;
        rdfs:seeAlso  <https://tools.ietf.org/html/rfc7519#section-4.1.5> ;
        rdfs:comment  "The 'aud' (audience) claim identifies the recipients that the JWT is intended for."@en .

ids:referringConnector
        rdf:type      owl:DatatypeProperty ;
        rdfs:label    "referringConnector"@en ;
        rdfs:domain   ids:JwtPayload ;
        rdfs:range    ids:Connector ;
        rdfs:comment  "The RDF connector entity as referred to by the DAT, with its URI included as the value. The value MUST be its accessible URI."@en .

ids:scope  rdf:type   owl:DatatypeProperty ;
        rdfs:label    "scope"@en ;
        rdfs:domain   ids:JwtPayload ;
        rdfs:range    xsd:string ;
        rdfs:seeAlso  <https://tools.ietf.org/html/rfc7519#section-4.1.2> ;
        rdfs:comment  "Currently, the scope is limited to 'Connector' but can be used for scoping purposes in the future. Scope is currently fixed to 'https://w3id.org/idsa/core/Connector'."@en .

ids:sub  rdf:type     owl:DatatypeProperty ;
        rdfs:label    "sub"@en ;
        rdfs:domain   ids:JwtPayload ;
        rdfs:range    xsd:string ;
        rdfs:seeAlso  <https://tools.ietf.org/html/rfc7519#section-4.1.2> ;
        rdfs:comment  "The 'sub' (subject) claim identifies the principal that is the subject of the JWT."@en .

ids:tokenValue  rdf:type  owl:DatatypeProperty ;
