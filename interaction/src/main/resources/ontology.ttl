@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix ids:   <https://w3id.org/idsa/core/> .

ids:AppRoute  rdf:type  owl:Class ;
        rdfs:label    "App Route"@en ;
        rdfs:comment  "\n    ids:AppRoute describes the communication between data apps (such as in a data processing pipeline) inside an IDS Connector.\n    An ids:AppRoute consist of multiple app endpoints which exchange data. The communication pipeline between two adjacent data apps is represented by the ids:RouteStep class.\n    ids:AppRoute also holds the information about the app endpoints, which are at the start and end of the whole processing pipeline.\n    "@en .

ids:RouteStep  rdf:type  owl:Class ;
        rdfs:subClassOf  ids:AppRoute ;
        rdfs:label       "Route step"@en ;
        rdfs:comment     "RouteSteps hold information about a communication pipelines between apps. A RouteStep should hold\n    one value for the ids:appRouteStart and/or ids:appRouteEnd properties, so that communication between app endpoints remain distinguishable.\n    "@en .

ids:routeDeployMethod
        rdf:type      owl:DatatypeProperty ;
        rdfs:label    "route deploy method"@en ;
        rdfs:domain   ids:AppRoute ;
        rdfs:range    xsd:string ;
        rdfs:comment  "Supplementary information about, e.g., the technology used, for routes." .

ids:routeDescription  rdf:type  owl:DatatypeProperty ;
        rdfs:label    "route description "@en ;
        rdfs:domain   ids:AppRoute ;
        rdfs:range    xsd:string ;
        rdfs:comment  "Route description."@en .

ids:routeConfiguration
        rdf:type      owl:DatatypeProperty ;
        rdfs:label    "route configuration"@en ;
        rdfs:domain   ids:AppRoute ;
        rdfs:range    xsd:string ;
        rdfs:comment  "Configuration details (e.g., contents of a config file) used to configure the route. Should be Base64 encoded."@en .

ids:hasSubRoute  rdf:type  owl:ObjectProperty ;
        rdfs:label    "sub route"@en ;
        rdfs:domain   ids:AppRoute ;
        rdfs:range    ids:RouteStep ;
        rdfs:comment  "Route between two or more app endpoints inside a app routing pipeline."@en .

ids:appRouteStart  rdf:type  owl:ObjectProperty ;
        rdfs:label    "app route start"@en ;
        rdfs:domain   ids:AppRoute ;
        rdfs:range    ids:Endpoint ;
        rdfs:comment  "Start of a route."@en .

ids:appRouteEnd  rdf:type  owl:ObjectProperty ;
        rdfs:label    "app route end"@en ;
        rdfs:domain   ids:AppRoute ;
        rdfs:range    ids:Endpoint ;
        rdfs:comment  "End of a route."@en .

ids:appRouteOutput  rdf:type  owl:ObjectProperty ;
        rdfs:domain   ids:AppRoute ;
        rdfs:range    ids:Resource ;
        rdfs:label    "app route output"@en ;
        rdfs:comment  "ids:Resource that corresponds to the output of an app route.\nRationale: whereas an ids:Endpoint can have an ids:endpointArtifact, it is additionally important that the data flow inside the ids:Connector / ids:ConfigurationModel is aware of the ids:ContractOffer (a property of ids:Resource!) associated with the data coming into the Connector or going out of the Connector.\nUse: Whenever properties of this Resource change, it may make sense to notify the ids:broker(s) configured for this AppRoute.\nNote: In an ids:AppRoute that is the sequence of multiple ids:RouteSteps, the output resource of one step serves immediately as the input that is processed by the subsequent step(s).  An ids:SystemAdapter typically does not change the ids:Resource, but might rather make it available in a different dcat:Distribution.  An ids:SmartDataApp might replate its input(s) by an entirely different (e.g., fused) resource.\n"@en .

ids:appRouteBroker  rdf:type  owl:ObjectProperty ;
        rdfs:domain   ids:AppRoute ;
        rdfs:range    ids:Broker ;
        rdfs:label    "app route broker"@en ;
        rdfs:comment  "One or more ids:Broker(s) to which changes/updates of this route's ids:appRouteOutput ids:Resource are announced.\nThis should be a subset of those Brokers that have been configured as ids:configuredBrokers in the ids:ConfigurationModel of which this is an ids:appRoute.\n"@en .

ids:appRouteClearingHouse
        rdf:type      owl:ObjectProperty ;
        rdfs:domain   ids:AppRoute ;
        rdfs:range    ids:ClearingHouse ;
        rdfs:label    "app route broker"@en ;
        rdfs:comment  "One or more ids:ClearingHouse(s) to which information of this route's data exchange are notified.\nThis should be a subset of clearing houses listed in the corresponding ids:ClearingHouseCatalog for a given configuration model. The Configuration Model ids:configuredClearingHouse in the ids:ConfigurationModel of which this is an ids:appRoute.\n"@en .

ids:requiredDataApp  rdf:type  owl:ObjectProperty ;
        rdfs:domain   ids:RouteStep ;
        rdfs:range    ids:AppRepresentation ;
        rdfs:label    "requiredDataApp"@en ;
        rdfs:comment  "Refers to the concrete app representation (ids:AppRepresentation) for a data app (ids:DataApp) which is deployed in the Connector maintained by the Configuration Model and\n    whoose endpoint (ids:AppEndpoint) is used in this ids:RouteStep. The referred app representation should be part of the catalog of AppResources in the\n    corresponding Configuration Model (cf. ids:configuredAppResource property in class ids:ConfigurationModel).\n    "@en .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix dct:   <http://purl.org/dc/terms/> .
@prefix ids:   <https://w3id.org/idsa/core/> .

ids:Endpoint  rdf:type  owl:Class ;
        rdfs:label    "Endpoint"@en ;
        rdfs:comment  "Access point of data exchange, service interaction and service description."@en .

ids:ConnectorEndpoint
        rdf:type         owl:Class ;
        rdfs:subClassOf  ids:Endpoint ;
        rdfs:label       "Connector Endpoint"@en ;
        rdfs:comment     "Connector-specific endpoint exposing Artifacts."@en .

ids:AppEndpoint  rdf:type  owl:Class ;
        rdfs:subClassOf  ids:Endpoint ;
        rdfs:label       "DataApp Endpoint"@en ;
        rdfs:comment     "Description of endpoints a DataApp offers."@en .

ids:AppEndpointType  rdf:type  owl:Class ;
        rdfs:label    "App endpoint type"@en ;
        rdfs:comment  "Class of endpoint types a data app offers."@en .

ids:GenericEndpoint  rdf:type  owl:Class ;
        rdfs:subClassOf  ids:Endpoint ;
        rdfs:label       "Generic Endpoint"@en ;
        rdfs:comment     "Generic endpoints describe non-IDS-specific endpoints, e.g., to integrate external services."@en .

ids:IdsProtocolSpecification
        rdf:type      owl:Class ;
        rdfs:label    "Protocol Specification" ;
        rdfs:comment  "List of all defined Protocol Specifications, for instance IDS-CP or IDS over Multipart."@en .

ids:accessURL  rdf:type  owl:DatatypeProperty ;
        rdfs:label    "access URL"@en ;
        rdfs:domain   ids:Endpoint ;
        rdfs:range    xsd:anyURI ;
        rdfs:comment  "Access URL of an endpoint."@en ;
        rdfs:seeAlso  <https://tools.ietf.org/html/rfc3986#section-3> .

ids:endpointInformation
        rdf:type      owl:DatatypeProperty ;
        rdfs:domain   ids:Endpoint ;
        rdfs:range    xsd:string ;
        rdfs:label    "endpoint information"@en ;
        rdfs:comment  "Human-readable information and description of the endpoint."@en .

ids:endpointDocumentation
        rdf:type      owl:DatatypeProperty ;
        rdfs:domain   ids:Endpoint ;
        rdfs:range    xsd:anyURI ;
        rdfs:label    "endpoint documentation"@en ;
        rdfs:comment  "IRI reference to a documentation of the endpoint, e.g., reference to an OpenAPI-based documentation."@en .

ids:endpointArtifact  rdf:type  owl:ObjectProperty ;
        rdfs:label    "endpoint artifact"@en ;
        rdfs:domain   ids:ConnectorEndpoint ;
        rdfs:range    ids:Artifact ;
        rdfs:comment  "Content instance mediated via this individual endpoint (1:1 mapping)."@en .

ids:path  rdf:type    owl:DatatypeProperty ;
        rdfs:label    "path"@en ;
        rdfs:domain   ids:Endpoint ;
        rdfs:range    xsd:string ;
        rdfs:comment  "Relative path, topic or queue at which the content is published by the related host."@en .

ids:inboundPath  rdf:type  owl:DatatypeProperty ;
        rdfs:label    "inbound topic"@en ;
        rdfs:domain   ids:Endpoint ;
        rdfs:range    xsd:string ;
        rdfs:comment  "Relative path used for inbound communication via this endpoint, i.e., input messages and faults only."@en .

ids:outboundPath  rdf:type  owl:DatatypeProperty ;
        rdfs:label    "outbound topic"@en ;
        rdfs:domain   ids:Endpoint ;
        rdfs:range    xsd:string ;
        rdfs:comment  "Relative path used for outbound communication via this endpoint, i.e., output messages and faults only."@en .

ids:apiSpecifiation  rdf:type  owl:ObjectProperty ;
        rdfs:label    "follows API specification" ;
        rdfs:comment  "Declares which endpoint specification is supported by this IDS endpoint." ;
        rdfs:domain   ids:Endpoint ;
        rdfs:range    ids:IdsProtocolSpecification .

ids:appEndpointType  rdf:type  owl:ObjectProperty ;
        rdfs:domain   ids:AppEndpoint ;
        rdfs:range    ids:AppEndpointType ;
        rdfs:label    "App endpoint type"@en ;
        rdfs:comment  "Type of an app endpoint. Data apps have different endpoint types, e.g., for input, output or health status."@en .

ids:appEndpointPort  rdf:type  owl:DatatypeProperty ;
        rdfs:domain   ids:AppEndpoint ;
        rdfs:range    xsd:integer ;
        rdfs:label    "app endpoint port"@en ;
        rdfs:comment  "Port used to access the endpoint."@en .

ids:appEndpointMediaType
        rdf:type      owl:ObjectProperty ;
        rdfs:domain   ids:AppEndpoint ;
        rdfs:range    dct:MediaType ;
        rdfs:label    "app endpoint media type"@en ;
        rdfs:comment  "Mediatype, such as IANA types, of the data an endpoint expects or returns ."@en .

ids:genericEndpointAuthentication
        rdf:type      owl:ObjectProperty ;
        rdfs:domain   ids:GenericEndpoint ;
        rdfs:range    ids:BasicAuthentication ;
        rdfs:label    "generic endpoint authentication"@en ;
        rdfs:comment  "Authentication credentials for generic endpoints. "@en .

ids:genericEndpointConfiguration
        rdf:type      owl:DatatypeProperty ;
        rdfs:domain   ids:GenericEndpoint ;
        rdfs:range    xsd:string ;
        rdfs:label    "generic endpoint configuration"@en ;
        rdfs:comment  "Endpoint-specific configuration details (e.g., contents of a config file) used for the endpoint."@en .

ids:appEndpointProtocol
        rdf:type      owl:DatatypeProperty ;
        rdfs:domain   ids:AppEndpoint ;
        rdfs:range    xsd:string ;
        rdfs:label    "app endpoint protocol"@en ;
        rdfs:comment  "Protocol used to communicate with the app endpoint."@en .
