@prefix ids:   <https://w3id.org/idsa/core/> .

ids:Message  rdf:type  owl:Class ;
        rdfs:label    "Message"@en ;
        rdfs:comment  "Metadata describing payload exchanged by interacting Connectors."@en .

ids:modelVersion  rdf:type  owl:DatatypeProperty ;
        rdfs:label    "model version"@en ;
        rdfs:domain   ids:Message ;
        rdfs:range    xsd:string ;
        rdfs:comment  "Version of the Information Model against which the Message should be interpreted."@en .

ids:correlationMessage
        rdf:type      owl:ObjectProperty ;
        rdfs:label    "consumer connector"@en ;
        rdfs:domain   ids:Message ;
        rdfs:range    ids:Message ;
        rdfs:comment  "Correlated message, e.g., a response to a previous request."@en .

ids:issuerConnector  rdf:type  owl:DatatypeProperty ;
        rdfs:label    "issuer connector"@en ;
        rdfs:domain   ids:Message ;
        rdfs:range    ids:Connector ;
        rdfs:comment  "The Connector which is the origin of the message."@en .

ids:recipientConnector
        rdf:type      owl:ObjectProperty ;
        rdfs:label    "recipient connector"@en ;
        rdfs:domain   ids:Message ;
        rdfs:range    ids:Connector ;
        rdfs:comment  "The Connector which is the recipient of the message."@en .

ids:senderAgent  rdf:type  owl:DatatypeProperty ;
        rdfs:label    "sender agent"@en ;
        rdfs:domain   ids:Message ;
        rdfs:range    foaf:Agent ;
        rdfs:comment  "The Agent which initiated the Message."@en .

ids:recipientAgent  rdf:type  owl:DatatypeProperty ;
        rdfs:label    "recipient agent"@en ;
        rdfs:domain   ids:Message ;
        rdfs:range    foaf:Agent ;
        rdfs:comment  "The Agent for which the Mesaage is intended."@en .

ids:securityToken  rdf:type  owl:ObjectProperty ;
        rdfs:label    "Security token"@en ;
        rdfs:domain   ids:Message ;
        rdfs:range    ids:DynamicAttributeToken ;
        rdfs:comment  "A token representing a claim that the message sender supports a certain security profile."@en .

ids:authorizationToken
        rdf:type      owl:ObjectProperty ;
        rdfs:label    "Authorization token"@en ;
        rdfs:domain   ids:Message ;
        rdfs:range    ids:Token ;
        rdfs:comment  "An authorization token like JSON Web Token."@en .

ids:transferContract  rdf:type  owl:ObjectProperty ;
        rdfs:label    "transferContract"@en ;
        rdfs:domain   ids:Message ;
        rdfs:range    ids:Contract ;
        rdfs:comment  "The contract which is (or will be) the legal basis of the data transfer."@en .

ids:contentVersion  rdf:type  owl:DatatypeProperty ;
        rdfs:label    "contentVersion"@en ;
        rdfs:domain   ids:Message ;
        rdfs:range    xsd:string ;
        rdfs:comment  "Version of the content in the payload. Use digits and semantic versioning pattern like MAJOR.MINOR.PATCH."@en .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct:   <http://purl.org/dc/terms/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix ids:   <https://w3id.org/idsa/core/> .

ids:Proxy  rdf:type   owl:Class ;
        rdfs:label    "Proxy"@en ;
        rdfs:comment  "Holds Proxy information."@en ;
        rdfs:seeAlso  <https://tools.ietf.org/html/rfc2068> .

ids:proxyURI  rdf:type  owl:DatatypeProperty ;
        rdfs:domain   ids:Proxy ;
        rdfs:range    xsd:anyURI ;
        rdfs:seeAlso  <https://www.gnu.org/software/emacs/manual/html_node/url/Proxies.html> ;
        rdfs:label    "proxy URI"@en ;
        rdfs:comment  "URI of the proxy server."@en .

ids:noProxy  rdf:type  owl:DatatypeProperty ;
        rdfs:domain   ids:Proxy ;
        rdfs:range    xsd:anyURI ;
        rdfs:seeAlso  <https://www.gnu.org/software/emacs/manual/html_node/url/Proxies.html> ;
        rdfs:label    "no proxy"@en ;
        rdfs:comment  "URLs that should be excluded from proxying.GNU emcs documentation contains a good explanation for the usage of noProxy."@en .

ids:proxyAuthentication
        rdf:type      owl:DatatypeProperty ;
        rdfs:domain   ids:Proxy ;
        rdfs:range    ids:BasicAuthentication ;
        rdfs:label    "proxy Authentication"@en ;
        rdfs:comment  "authentication information for the proxy"@en .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct:   <http://purl.org/dc/terms/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix odrl:  <http://www.w3.org/ns/odrl/2/> .
@prefix ids:   <https://w3id.org/idsa/core/> .

ids:actionRefinement  rdf:type  owl:ObjectProperty ;
        rdfs:label    "action refinement"@en ;
        rdfs:domain   odrl:Action ;
        rdfs:range    odrl:Constraint ;
        rdfs:comment  "Constraint that refines an Action."@en .

ids:pxpEndpoint  rdf:type  owl:ObjectProperty ;
        rdfs:label    "has PXP endpoint"@en ;
        rdfs:domain   odrl:Action ;
        rdfs:range    ids:PXP ;
        rdfs:comment  "The reference to the PXP which operates a specific action, URI or reference to an associated PXP interface."@en .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
